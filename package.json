{
  "private": true,
  "name": "electron-chimera-project",
  "version": "1.0.0",
  "main": "./electron-main.js",
  "electronmon": {
    "patterns": [
      "./src/**/*.*",
      "./electron/**/*.*"
    ]
  },
  "description": "基于chimera||koa2搭建的electron项目",
  "scripts": {
    "dev:web:local": "cross-env CHIMERA_RUNTIME=local spa-build-tools dev",
    "dev:client:local": "cross-env PROCESS_ENV=development electronmon electron-main.js",
    "dev:client:prod": "cross-env PROCESS_ENV=production electronmon electron-main.js",
    "build:dist": "spa-build-tools build",
    "build:mac:only": "electron-builder --universal --config=electron-builder-mac.json",
    "build:win:only": "electron-builder --windows --ia32 --config=electron-builder-win.json",
    "build:mac:dist": "npm run build:dist && npm run build:mac:only",
    "build:win:dist": "npm run build:dist && npm run build:win:only",
    "ico": "png-to-ico ./build/icon.png > ./build/installerIcon.ico",
    "icns": "png2icons ./build/icon.png ./build/icon -icns -bc -i"
  },
  "dependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "7.15.3",
    "babel-plugin-import": "1.13.5",
    "babel-plugin-module-resolver": "4.1.0",
    "cheerio": "^1.0.0-rc.12",
    "cross-spawn": "^7.0.3",
    "exceljs": "^4.3.0",
    "path-to-regexp": "6.2.0"
  },
  "devDependencies": {
    "@cyber-chimera/spa-build-tools": "^5.0.0",
    "@koa/router": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "axios": "^0.21.1",
    "babel-eslint": "10.1.0",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "electron": "12.0.18",
    "electron-builder": "22.14.13",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "4.3.9",
    "electronmon": "^2.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "koa": "2.13.1",
    "koa-cookie": "1.0.0",
    "koa-proxies": "0.12.2",
    "koa-static": "5.0.0",
    "png-to-ico": "^2.1.2",
    "png2icons": "^2.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-i18next": "^11.15.7",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^6.2.2",
    "ssr-window": "4.0.2",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-standard": "18.3.0"
  }
}
